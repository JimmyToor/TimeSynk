<%= render @proposal_availability %>

<div>
  <%= link_to "Edit this proposal availability", edit_proposal_availability_path(@proposal_availability) %> |
  <%= link_to "Back to proposal availabilities", game_proposal_path(@proposal_availability.game_proposal) %>

  <%= button_to "Destroy this proposal availability", @proposal_availability, method: :delete %>
</div>

<div id="calendar" data-controller="calendar" data-calendar-interactive-value="true" data-calendar-target="calendar" data-calendar-availability-id=<%= @proposal_availability.availability_id %>>
</div>