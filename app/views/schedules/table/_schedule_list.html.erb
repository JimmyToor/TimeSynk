<%# locals: (schedules:) %>
<div class="overflow-x-auto text-center">
  <table class="w-full text-sm text-left text-gray-500 dark:text-gray-400">
    <thead class="text-xs text-gray-700 uppercase bg-tertiary-50 dark:bg-gray-700 dark:text-gray-400">
      <tr class="text-center">
        <th></th>
        <th scope="col" class="px-4 py-3 text-left">Schedule</th>
        <th scope="col" class="px-4 py-3 hidden md:table-cell">Description</th>
        <th scope="col" class="px-4 py-3 hidden lg:table-cell">Frequency</th>
        <th scope="col" class="px-4 py-3 hidden xl:table-cell">Start Date</th>
        <th scope="col" class="px-4 py-3 hidden xl:table-cell">End Date</th>
        <th scope="col" class="px-4 py-3 hidden 2xl:table-cell">Duration</th>
      </tr>
    </thead>
    <tbody data-controller="element-toggle" data-element-toggle-display-style-value="table-row">
      <% schedules.each do |schedule| %>
        <%= render partial:"schedules/table/schedule_table_row", locals: {schedule: schedule} %>
      <% end %>
    </tbody>
  </table>
</div>
<%= render partial: "shared/table_nav" %>