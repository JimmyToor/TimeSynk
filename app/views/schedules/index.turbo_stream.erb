<% if @included_schedules.present? %>
  <%= turbo_stream.update("schedule_table_form",
                          partial:"schedules/table/schedule_table_form",
                          locals: {availability:@availability, schedules:@schedules, included_schedules: @included_schedules}) %>
<% else %>
  <%= turbo_stream.update("schedule_table",
                        partial:"schedules/table/schedule_table",
                        locals: {schedules:@schedules}) %>
<% end %>
