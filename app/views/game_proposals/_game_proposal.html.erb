<div id="<%= dom_id game_proposal %>">
  <div>
    <%= display_game_art_and_name(game_proposal.game) %>
  </div>

  <p>
    <strong>Group:</strong>
    <%= game_proposal.group_id %>
  </p>

  <p>
    <strong>Game ID:</strong>
    <%= game_proposal.game_id %>
  </p>

  <p>
    <strong>User:</strong>
    <%= game_proposal.user_id %>
  </p>

  <%= render 'game_proposals/vote_count', game_proposal: game_proposal %>

</div>
