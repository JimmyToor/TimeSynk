<% if local_assigns[:game_proposal].user_voted?(Current.user) %>
  <%= link_to "Edit Vote", edit_proposal_vote_path(Current.user.get_vote_for_proposal(game_proposal)),  data: { turbo_frame: "modal_frame"} %>
<% else %>
  <%= link_to "Cast Vote", new_game_proposal_proposal_vote_path(game_proposal), data: { turbo_frame: "modal_frame"} %>
<% end %>
