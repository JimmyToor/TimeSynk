<%# locals: (game_proposals:, game_proposal: nil) %>
<div class="flex rounded-lg dark:border-gray-600">
  <ul>
    <% game_proposals.each do |proposal| %>
      <li class="flex">
        <%= render proposal,
                   proposal_availability: proposal.get_user_proposal_availability(Current.user),
                   proposal_permission_set: proposal.make_permission_set(proposal.group.users.to_a) %>
        <%= card_link_to_game_proposal(proposal) %>
      </li>
    <% end %>
  </ul>
</div>
<%= render "shared/table_nav" %>