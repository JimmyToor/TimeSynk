<p style="color: green"><%= notice %></p>

<h1>Group memberships</h1>
<%= render "shared/notification", id: "group_memberships" %>
<% frame_path = group_group_memberships_path(group_id: group.id, format: :turbo_stream) %>

<div id="group_memberships">
  <% @group_memberships.each do |group_membership| %>
    <%= render group_membership %>
    <p>
      <%= link_to "Show this group membership", group_membership %>
    </p>
  <% end %>
</div>

<%= link_to "New group membership", new_group_group_membership_path %>
